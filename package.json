{
  "name": "push-notification-lambda",
  "version": "1.0.0",
  "description": "A lambda for sending push notifications to devices",
  "main": "app.js",
  "scripts": {
    "test": "mocha ./src/tests/unit",
    "clean": "rm -rf ./aws-sam/build",
    "build": "sam build --profile personal --region ap-southeast-2",
    "local": "sam local invoke --event events/event.json",
    "sam:deploy": "sam deploy --profile personal --region ap-southeast-2",
    "sam:rollback": "sam delete",
    "serverless:deploy": "serverless deploy function -f PushNotification"
  },
  "author": "Prue Phillips",
  "license": "ISC",
  "dependencies": {
    "@parse/node-apn": "^5.1.3",
    "aws-sdk": "^2.1397.0",
    "serverless": "^3.33.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "serverless-offline": "^12.0.4"
  }
}
